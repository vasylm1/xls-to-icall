Â  Â  Â  Â  Â  link.href = URL.createObjectURL(blob);
Â  Â  Â  Â  Â  link.style.display = 'inline-block';
Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  } catch (err) {
Â  Â  Â  Â  Â  showError(t.fileError + err.message);
Â  Â  Â  Â  } finally {
Â  Â  Â  Â  Â  loadingIndicator.style.display = 'none';
Â  Â  Â  Â  Â  convertBtn.disabled = false;
Â  Â  Â  Â  }
Â  Â  Â  };
