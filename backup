Â  Â  function switchTab(tab) {
Â  Â  Â  document.querySelectorAll('.tab').forEach(t => {
Â  Â  Â  Â  t.classList.remove('active');
Â  Â  Â  Â  t.setAttribute('aria-selected', 'false');
Â  Â  Â  });
Â  Â  Â  document.querySelectorAll('.tab-content').forEach(tc => {
Â  Â  Â  Â  tc.classList.remove('active');
Â  Â  Â  Â  tc.setAttribute('hidden', 'true');
Â  Â  Â  });
Â  Â  Â Â 
Â  Â  Â  document.querySelector(`.tab[onclick="switchTab('${tab}')"]`).classList.add('active');
