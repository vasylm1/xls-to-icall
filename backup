Â  Â  function formatDate(dateStr) {
Â  Â  Â  if (!dateStr) return '';
Â  Â  Â Â 
Â  Â  Â  if (typeof dateStr === 'string') {
Â  Â  Â  Â  const plMatch = dateStr.match(/(\d{2})\.(\d{2})\.(\d{4}) (\d{2}):(\d{2})/);
Â  Â  Â  Â  if (plMatch) {
Â  Â  Â  Â  Â  const [_, day, month, year, hour, minute] = plMatch;
Â  Â  Â  Â  Â  return `${year}${month}${day}T${hour}${minute}00`;
Â  Â  Â  Â  }
Â  Â  Â  }
Â  Â  Â Â 
Â  Â  Â  if (typeof dateStr === 'number') {
Â  Â  Â  Â  const date = new Date(Math.round((dateStr - 25569) * 86400 * 1000));
Â  Â  Â  Â  return date.toISOString().replace(/[-:]/g, '').split('.')[0] + 'Z';
Â  Â  Â  }
Â  Â  Â Â 
Â  Â  Â  const date = new Date(dateStr);
Â  Â  Â  if (!isNaN(date.getTime())) {
Â  Â  Â  Â  return date.toISOString().replace(/[-:]/g, '').split('.')[0] + 'Z';
Â  Â  Â  }
Â  Â  Â Â 
Â  Â  Â  return '';
Â  Â  }

Â  Â  function escapeIcalText(text) {
Â  Â  Â  return String(text || '')
Â  Â  Â  Â  .replace(/\\/g, '\\\\')
Â  Â  Â  Â  .replace(/,/g, '\\,')
Â  Â  Â  Â  .replace(/;/g, '\\;')
Â  Â  Â  Â  .replace(/\n/g, '\\n');
Â  Â  }

Â  Â  function validateExcelHeaders(headers) {
Â  Â  Â  const requiredColumns = [
Â  Â  Â  Â  'data rozpoczÄ™cia',
Â  Â  Â  Â  'data zakoÅ„czenia',
Â  Â  Â  Â  'nazwa peÅ‚na przedmiotu'
Â  Â  Â  ];
Â  Â  Â Â 
Â  Â  Â  const lowerHeaders = headers.map(h => String(h).toLowerCase().trim());
Â  Â  Â  return requiredColumns.every(col =>Â 
Â  Â  Â  Â  lowerHeaders.some(h => h.includes(col.toLowerCase()))
Â  Â  Â  );
Â  Â  }

