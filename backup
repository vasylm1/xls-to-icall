Â  Â  Â  Â  Â  Â  throw new Error(t.invalidFileError);
Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  let ics = `BEGIN:VCALENDAR\nVERSION:2.0\nPRODID:-//AEH Schedule Converter//EN\nCALSCALE:GREGORIAN\n`;
Â  Â  Â  Â  Â  ics += `BEGIN:VTIMEZONE\nTZID=Europe/Warsaw\nBEGIN:STANDARD\nDTSTART:19701025T030000\nTZOFFSETFROM:+0200\nTZOFFSETTO:+0100\nTZNAME:CET\nEND:STANDARD\nBEGIN:DAYLIGHT\nDTSTART:19700329T020000\nTZOFFSETFROM:+0100\nTZOFFSETTO:+0200\nTZNAME:CEST\nEND:DAYLIGHT\nEND:VTIMEZONE\n`;
Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  for (let i = 1; i < json.length; i++) {
Â  Â  Â  Â  Â  Â  const row = json[i];
Â  Â  Â  Â  Â  Â  if (!row || row.length < 10) continue;
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  const start = formatDate(row[0]);
Â  Â  Â  Â  Â  Â  const end = formatDate(row[1]);
Â  Â  Â  Â  Â  Â  const hours = row[2];
Â  Â  Â  Â  Â  Â  const summary = row[4];
Â  Â  Â  Â  Â  Â  const groupThematic = row[5];
Â  Â  Â  Â  Â  Â  const groupClass = row[6];
Â  Â  Â  Â  Â  Â  const lecturer = row[7];
Â  Â  Â  Â  Â  Â  const stageType = row[8];
Â  Â  Â  Â  Â  Â  const deanGroups = row[9];
Â  Â  Â  Â  Â  Â  const room = row[10] || '';
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  if (!start || !summary) continue;

Â  Â  Â  Â  Â  Â  const description = escapeIcalText([
Â  Â  Â  Â  Â  Â  Â  groupThematic && `Grupa tematyczna: ${groupThematic}`,
Â  Â  Â  Â  Â  Â  Â  groupClass && `Grupa zajÄ™ciowa: ${groupClass}`,
Â  Â  Â  Â  Â  Â  Â  lecturer && `WykÅ‚adowca: ${lecturer}`,
Â  Â  Â  Â  Â  Â  Â  hours && `Godziny: ${hours}`,
Â  Â  Â  Â  Â  Â  Â  stageType && `Etap: ${stageType}`,
Â  Â  Â  Â  Â  Â  Â  deanGroups && `Grupy: ${deanGroups.toString().replace(/\n/g, ', ')}`
Â  Â  Â  Â  Â  Â  ].filter(Boolean).join("\\n"));
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  const location = room.toLowerCase().includes('teams')Â 
Â  Â  Â  Â  Â  Â  Â  ? 'Microsoft Teams'Â 
Â  Â  Â  Â  Â  Â  Â  : room.replace(/Teams/g, '').trim();
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  ics += `BEGIN:VEVENT\n`;
Â  Â  Â  Â  Â  Â  ics += `DTSTART;TZID=Europe/Warsaw:${start}\n`;
Â  Â  Â  Â  Â  Â  ics += `DTEND;TZID=Europe/Warsaw:${end}\n`;
Â  Â  Â  Â  Â  Â  ics += `SUMMARY:${escapeIcalText(summary)}\n`;
Â  Â  Â  Â  Â  Â  if (description) ics += `DESCRIPTION:${description}\n`;
Â  Â  Â  Â  Â  Â  if (location) ics += `LOCATION:${escapeIcalText(location)}\n`;
